# EditorConfig helps maintain consistent coding styles across editors
root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
trim_trailing_whitespace = true
indent_style = space
indent_size = 4

[*.swift]
indent_size = 4
indent_style = space

[*.{sh,py,yml,yaml,json,md}]
indent_size = 2
